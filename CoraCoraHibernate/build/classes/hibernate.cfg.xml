<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.OracleDialect</property>
    <property name="hibernate.connection.driver_class">oracle.jdbc.OracleDriver</property>
    <property name="hibernate.connection.url">jdbc:oracle:thin:@localhost:1521:XE</property>
    <property name="hibernate.connection.username">BD_CORACORA</property>
    <property name="hibernate.connection.password">oracle</property>
    <mapping resource="Modelo/Salidas.hbm.xml"/>
    <mapping resource="Modelo/Cobradores.hbm.xml"/>
    <mapping resource="Modelo/Quejas.hbm.xml"/>
    <mapping resource="Modelo/Vehiculos.hbm.xml"/>
    <mapping resource="Modelo/Asociados.hbm.xml"/>
    <mapping resource="Modelo/Usuarios.hbm.xml"/>
    <mapping resource="Modelo/Choferes.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
